## start of getRightsAssignment macro
#macro(getRightsAssignment $rightsObj)
	<p class="shorttext">
		<span class="reqStar">*</span>
		<label for="$rightsObj.fieldNameAuthorship">
			$rightsObj.getString("$rightsObj.keyCcAuthorshipLabel")
		</label>	
		<select name="$rightsObj.fieldNameAuthorship" id ="$rightsObj.fieldNameAuthorship" onchange="javascript:updateRightsAssignment(this, '$rightsObj.name');">
			#foreach ($option in $rightsObj.ccAuthorshipKeys)
				<option value="$option"#if("$!{rightsObj.ccOwnership}" == "$option") selected="selected"#end>$!{rightsObj.getString("$option")}</option>
			#end
		</select>
		##if ($!ccInfoUrl)
		##	(<a href="$ccInfoUrl" target="_blank" title="[$tlang.getString('gen.newwin')]"><span class="skip"> [$tlang.getString('gen.newwin')]</span>$tlang.getString("edit.fairuse")</a>)
		##end
	</p>
	<div id="copyright_div_1_$!{rightsObj.name}">
		#if("$!{rightsObj.ccOwnership}" == "$!{rightsObj.keyAuthorMe}")
			<p class="shorttext"> 
				<span class="reqStar">*</span> 
				<label for="$!{rightsObj.fieldNameMyTerms}">$rightsObj.getString($rightsObj.keyMyTermsLabel)</label>
				<select name="$!{rightsObj.fieldNameMyTerms}" id=\"$!{rightsObj.FieldNameMyTerms}" onchange="javascript:updateRightsAssignment(this, '$rightsObj.name');">
				#foreach ($option in $rightsObj.myTermsKeys)
					<option value="$option"#if("$!{rightsObj.ccTerms}" == "$option") selected="selected"#end>$!{rightsObj.getString("$option")}</option>
				#end
				</select> 
			</p>
		#elseif("$!{rightsObj.ccOwnership}" == "$!{rightsObj.keyAuthorOther}")
			<p class="shorttext"> 
				<span class="reqStar">*</span> 
				<label for="$!{rightsObj.fieldNameCcOtherTerms}">$rightsObj.getString("$rightsObj.")</label>
				<select name="$!{rightsObj.fieldNameCcOtherTerms}" id="$!{rightsObj.FieldNameCcOtherTerms}" onchange="javascript:updateRightsAssignment(this, '$rightsObj.name');">
					#foreach ($option in $rightsObj.otherTermsKeys))
						<option value="$option"#if("$!{rightsObj.otherLicense}" == "$option") selected="selected"#end>$!{rightsObj.getString("$option")}</option>
					#end
				</select> 
			</p> 
		#end
	</div>
	<div id="copyright_div_2_$!{rightsObj.name}">
		#if("$!{rightsObj.ccOwnership}" == "$!{rightsObj.keyAuthorMe}")
			#if("$!{rightsObj.ccTerms}" == "$!{rightsObj.keyMyCopyright}")
				<table border="0"> 
					<tr> 
						<td style="width:180px;padding-left:1em;vertical-align:top"> 
						</td>
						<td> 
						</td> 
						<td style="vertical-align:top"> 
							<label for="$!{rightsObj.fieldNameCcRightsYear}">$rightsObj.getString("$!{rightsObj.keyCopyrightYearLabel}")</label> 
						</td>
						<td style="vertical-align:top"> 
							<label for="$!{rightsObj.fieldNameCcRightsOwner}">$rightsObj.getString("$!{rightsObj.keyCopyrightOwnerLabel}")</label> 
						</td> 
					</tr>
					<tr> 
						<td style="width:180px;padding-left:1em;vertical-align:top"> 
						</td> 
						<td>&copy;</td>
						<td style="vertical-align:top"> 
							<input type="text" name="$!{rightsObj.fieldNameCcRightsYear}" id="$!{rightsObj.fieldNameCcRightsYear}" size="5" value="#if("$!{rightsObj.ccRightsYear}" == "")$!{rightsObj.defaultCopyrightYear}#else$!{rightsObj.ccRightsYear}#end" /> 
						</td> 
						<td style="vertical-align:top"> 
							<input type="text" name="$!{rightsObj.fieldNameCcRightsOwner}" id="$!{rightsObj.fieldNameCcRightsOwner}" size="40" value="#if("$!{rightsObj.ccRightsOwner}" == "")$!{rightsObj.defaultCopyrightOwner}#else$!{rightsObj.ccRightsOwner}#end" /> 
						</td> 
					</tr> 
				</table>
			#elseif("$!{rightsObj.ccTerms}" == "$!{rightsObj.keyCreativeCommons}")
				<div class="labelindnt">
					<h5>$rightsObj.getString("$!{rightsObj.keyCcCommercialLabel}")</h5>
					<p class="checkbox">
						<input type="radio" name="$rightsObj.fieldNameCcCommercial" id="$!{rightsObj.name}$!{rightsObj.keyCcCommercialYes}" value="$!{rightsObj.keyCcCommercialYes}" #if("$!{rightsObj.ccCommercial}" == "$!{rightsObj.keyCcCommercialYes}")checked="checked" #end/>
						<label for="$!{rightsObj.name}$!{rightsObj.keyCcCommercialYes}">$rightsObj.getString("$!{rightsObj.keyCcCommercialYes}")</label>
					</p>
					<p class="checkbox">
						<input type="radio" name="$rightsObj.fieldNameCcCommercial" id="$!{rightsObj.name}$!{rightsObj.keyCcCommercialNo}" value="$!{rightsObj.keyCcCommercialNo}" #if("$!{rightsObj.ccCommercial}" == "$!{rightsObj.keyCcCommercialNo}")checked="checked" #end/>
						<label for="$!{rightsObj.name}$!{rightsObj.keyCcCommercialNo}">$rightsObj.getString("$!{rightsObj.keyCcCommercialNo}")</label>
					</p>
				</div>
				<div class="labelindnt">
					<h5>$rightsObj.getString("$!{rightsObj.keyCcModifyLabel}")</h5>
					<p class="checkbox">
						<input type="radio" name="$rightsObj.fieldNameCcModify" id="$!{rightsObj.name}$!{rightsObj.keyCcModifyYes}" value="$!{rightsObj.keyCcModifyYes}" #if("$!{rightsObj.ccModify}" == "$!{rightsObj.keyCcModifyYes}")checked="checked" #end/>
						<label for="$!{rightsObj.name}$!{rightsObj.keyCcModifyYes}">$rightsObj.getString("$!{rightsObj.keyCcModifyYes}")</label>
					</p>
					<p class="checkbox">
						<input type="radio" name="$rightsObj.fieldNameCcModify" id="$!{rightsObj.name}$!{rightsObj.keyCcModifyShare}" value="$!{rightsObj.keyCcModifyShare}" #if("$!{rightsObj.ccModify}" == "$!{rightsObj.keyCcModifyShare}")checked="checked" #end/>
						<label for="$!{rightsObj.name}$!{rightsObj.keyCcModifyShare}">$rightsObj.getString("$!{rightsObj.keyCcModifyShare}")</label>
					</p>
					<p class="checkbox">
						<input type="radio" name="$rightsObj.fieldNameCcModify" id="$!{rightsObj.name}$!{rightsObj.keyCcModifyNo}" value="$!{rightsObj.keyCcModifyNo}" #if("$!{rightsObj.ccModify}" == "$!{rightsObj.keyCcModifyNo}")checked="checked" #end/>
						<label for="$!{rightsObj.name}$!{rightsObj.keyCcModifyNo}">$rightsObj.getString("$!{rightsObj.keyCcModifyNo}")</label>
					</p>
				</div>
			#end
		#elseif("$!{rightsObj.ccOwnership}" == "$!{rightsObj.keyAuthorOther}")
			
		#end
	</div>
	
#end 
## end of getRightsAssignment macro

## start of getCreativeCommons macro
#macro(getCreativeCommons $cItem $cIndex)
	<p class="shorttext">
		<span class="reqStar">*</span>
		<label for="owner$cIndex">
			$ccOwnershipLabel
		</label>	
		<select name="ccOwnership$cIndex" id ="ccOwnership$cIndex" onchange="javascript:updateCreativeCommons(this, '$cIndex');">
			#foreach ($option in $ccOwnershipList)
				<option value="$option"#if("$!{cItem.rightsownership}" == "$option") selected="selected"#end>$option</option>
			#end
		</select>
		#if ($!ccInfoUrl)
			(<a href="$ccInfoUrl" target="_blank" title="[$tlang.getString('gen.newwin')]"><span class="skip"> [$tlang.getString('gen.newwin')]</span>$tlang.getString("edit.fairuse")</a>)
		#end
	</p>
	<div id="copyright_div_1_$cIndex">
		#if("$!{cItem.rightsownership}" == "$ccOwnershipList.get(1)")
			<p class="shorttext"> 
				<span class="reqStar">*</span> 
				<label for="ccTerms$cIndex">$ccMyGrantLabel</label>
				<select name="ccTerms$cIndex" id=\"ccTerms$cIndex" onchange="javascript:updateCreativeCommons(this, '$cIndex');">
				#foreach ($option in $ccMyGrantOptions)
					<option value="$option"#if("$cItem.license" == "$option") selected="selected"#end>$option</option>
				#end
				</select> 
			</p>
		#elseif("$!{cItem.rightsownership}" == "$ccOwnershipList.get(2)")
			<p class="shorttext"> 
				<span class="reqStar">*</span> 
				<label for="ccTerms$cIndex">$ccOtherGrantLabel</label>
				<select name="ccTerms$cIndex" id="ccTerms$cIndex" onchange="javascript:updateCreativeCommons(this, '$cIndex');">
					#foreach ($option in $ccOtherGrantList)
						<option value="$option"#if("$cItem.license" == "$option") selected="selected"#end>$option</option>
					#end
				</select> 
			</p> 
		#end
	</div>
	<div id="copyright_div_2_$cIndex">
		#if("$!{cItem.license}" == "$ccMyGrantOptions.get(1)")
			<table border="0"> 
				<tr> 
					<td style="width:180px;padding-left:1em;vertical-align:top"> 
					</td>
					<td> 
					</td> 
					<td style="vertical-align:top"> 
						<label for="ccRightsYear$cIndex">$ccRightsYear</label> 
					</td>
					<td style="vertical-align:top"> 
						<label for="ccRightsOwner$cIndex">$ccRightsOwner</label> 
					</td> 
				</tr>
				<tr> 
					<td style="width:180px;padding-left:1em;vertical-align:top"> 
					</td> 
					<td>&copy;</td>
					<td style="vertical-align:top"> 
						<input type="text" name="ccRightsYear$cIndex" id="ccRightsYear$cIndex" size="5" value="#if($cItem.rightsyear)$cItem.rightsyear#else$ccThisYear#end" /> 
					</td> 
					<td style="vertical-align:top"> 
						<input type="text" name="ccRightsOwner$cIndex" id="ccRightsOwner$cIndex" value="#if($cItem.rightsowner)$cItem.rightsowner#else$ccThisUser#end" /> 
					</td> 
				</tr> 
			</table>
		#elseif("$!{cItem.license}" == "$ccMyGrantOptions.get(2)")
			<table border="0"> 
				<tr> 
					<td style="width:180px;padding-left:1em;vertical-align:top">
					</td> 
					<td style="vertical-align:top"> 
						<label for="ccCommercial$cIndex">$ccCommercialLabel</label>
						#foreach($option in $ccCommercialList)
							<input type="radio" name="ccCommercial$cIndex" id="ccCommercial${cIndex}_$validator.escapeJavascript(${option})" value="${option}"#if("$!{cItem.allowCommercial}" == "$option") checked="checked"#end>${option}</option>
						#end
					</td> 
				</tr> 
				<tr> 
					<td style="width:180px;padding-left:1em;vertical-align:top">
					</td> 
					<td style="vertical-align:top"> 
						<label for="ccModification$cIndex">$ccModificationLabel</label>
						#foreach($option in $ccModificationList)
							<input type="radio" name="ccModification$cIndex" id="ccModification${cIndex}_$validator.escapeJavascript(${option})" value="${option}"#if("$!{cItem.allowModifications}" == "$option") checked="checked"#end>${option}</option>
						#end
					</td> 
				</tr> 
			</table>
		#elseif("$!{cItem.license}" == "$ccMyGrantOptions.get(3)")
		#end
	</div>
	<div id="copyright_div_3_$cIndex">
	</div>
	
#end 
## end of getCreativeCommons macro

#macro(getPropertyValues $tags)
	#foreach($tag in $tags)
		#if($tag && $tag.label && "$!{tag.label}" != "")
			#set($indent = "")
			#if($tag.depth > 0)
				#set($indent = " indnt$!{tag.depth}")
			#end
			#if("$!NESTED" == "$!tag.widget")
				<div class="fakelabel$!{indent}">
					#if($tag.minCardinality > 0)<span class="reqStar">*</span>#end
					$tag.label
					#if($tag.canAddInstances())
						[<a href="#" onclick="javascript: document.getElementById('flow').value='addInstance'; document.getElementById('field').value='$!{tag.dottedname}'; submitform('createForm'); return false;" onkeypress="javascript: document.getElementById('flow').value='addInstance'; document.getElementById('field').value='$!{tag.dottedname}'; submitform('createForm'); return false;"  title="$tlang.getString("gen.add") $tag.label">+</a>]
					#end
					<input type="hidden"
						name="$!{tag.dottedname}" 
						id="$validator.escapeJavascript("$!{tag.dottedname}")" 
						value="" />
				</div>
			#else
				<div class="shorttext$!{indent}">
					<label for ="$validator.escapeJavascript("$!{tag.dottedname}")">
						#if($tag.minCardinality > 0)<span class="reqStar">*</span>#end
						$tag.label
						#if($tag.canAddInstances())
							[<a href="#" onclick="javascript: document.getElementById('flow').value='addInstance'; document.getElementById('field').value='$!{tag.dottedname}'; submitform('createForm'); return false;" onkeypress="javascript: document.getElementById('flow').value='addInstance'; document.getElementById('field').value='$!{tag.dottedname}'; submitform('createForm'); return false;"  title="$tlang.getString("gen.add") $tag.label">+</a>]
						#end
					</label>
					#if("$!tag.widget" == "$!STRING")
						<input type="text" size="$!{tag.length}"
							name="$!{tag.dottedname}" 
							id="$validator.escapeJavascript("$!{tag.dottedname}")" 
							value="$!{tag.value}" />
					#elseif("$!tag.widget" == "$!TEXTAREA")
						<textarea rows="4" cols="60" 
							name="$!{tag.dottedname}" 
							id="$validator.escapeJavascript("$!{tag.dottedname}")" >$validator.escapeHtmlTextarea($!{tag.value})</textarea>
					#elseif("$!tag.widget" == "$!WYSIWYG")
						<table><tr><td>
						<textarea rows="30" cols="80" 
							name="$!{tag.dottedname}" 
							id="$validator.escapeJavascript("$!{tag.dottedname}")" >$validator.escapeHtmlTextarea($!{tag.value}) </textarea>
						#chef_setupformattedtextarea("$validator.escapeJavascript($!{tag.dottedname})")
						</td></tr></table>
					#elseif("$!tag.widget" == "$!DATE")
						<input type="hidden" id="$validator.escapeJavascript("$!{tag.dottedname}")" value="" />
						#if($!{tag.value})
							#set($theValue = $!{tag.value})
						#else
							#set($theValue = $today)
						#end 
						#prop_dateSelectionWidget("${tag.dottedname}" $theValue)
					#elseif("$!tag.widget" == "$!DATETIME")
						<input type="hidden" id="$validator.escapeJavascript("$!{tag.dottedname}")" value="" />
						#if($!{tag.value})
							#set($theValue = $!{tag.value})
						#else
							#set($theValue = $today)
						#end 
						#prop_dateSelectionWidget("$!{tag.dottedname}" $theValue)
						#prop_timeSelectionWidget("$!{tag.dottedname}" $theValue)
					#elseif("$!tag.widget" == "$!TIME")
						<input type="hidden" id="$validator.escapeJavascript("$!{tag.dottedname}")" value="" />
						#if($!{tag.value})
							#set($theValue = $!{tag.value})
						#else
							#set($theValue = $today)
						#end 
						#prop_timeSelectionWidget("$!{tag.dottedname}" $theValue)
					#elseif("$!tag.widget" == "$!ENUM")
						<select name="$!{tag.dottedname}" 
							id="$validator.escapeJavascript("$!{tag.dottedname}")">
							#foreach($enum in $tag.enumeration)
								<option value="$enum"#if("$!{tag.value}" == "$enum") selected="selected"#end>$enum</option>
							#end
						</select>
					#elseif("$!tag.widget" == "$!BOOLEAN")
						<input type="checkbox" 
							name="$!{tag.dottedname}" 
							id="$validator.escapeJavascript("$!{tag.dottedname}")"
							#if("$!{tag.value}" == "$TRUE")checked="checked"#end
							value="true" />
					#elseif("$!tag.widget" == "$!INTEGER")
						<input type="text" size="$!{tag.length}"
							name="$!{tag.dottedname}" 
							id="$validator.escapeJavascript("$!{tag.dottedname}")" 
							value="$!{tag.value}" />
					#elseif("$!tag.widget" == "$!DOUBLE")
						<input type="text" size="$!{tag.length}"
							name="$!{tag.dottedname}" 
							id="$validator.escapeJavascript("$!{tag.dottedname}")" 
							value="$!{tag.value}" />
					#elseif("$!tag.widget" == "$!ANYURI")
						#set ($size = 0)
						#if (!$tag.values.isEmpty())
							#set ($props = false)
							#set ($size = $tag.values.size())
							<ul class="attachList">
								#foreach ($attachment in $tag.values)
									#set ($props = $attachment.Properties)
									#if (!$props)
										#if ($size > 0) #set ($size = $size - 1) #end
									#else
										#if ($props.getBooleanProperty($props.NamePropIsCollection))
											<li><img src = "#imageLink($contentTypeImageService.getContentTypeImage("folder"))" border="0" />
										#else
											<li><img src = "#imageLink($contentTypeImageService.getContentTypeImage($props.getProperty($props.NamePropContentType)))" border="0" />
										#end
										#set ($copyflag = false)
										#if ($props.getProperty("CHEF:copyrightalert"))
											#if ($props.getBooleanProperty("CHEF:copyrightalert"))
												#set ($copyflag = true)
											#end
										#end
				
										#if ($copyflag)
											## if there is a copyright alert, show the alert page first
											<a href="" onclick="openCopyrightWindow('${attachment.url}','copyrightAlertWindow','scrollbars=yes,menubar=yes,height=600,width=800,resizable=yes,toolbar=yes,location=yes,status=yes');return false">
											$validator.escapeHtml($attachment.Properties.getPropertyFormatted("DAV:displayname"))</a>&#169;
										#else
											<a href="$attachment.Url" target="_blank">
											$validator.escapeHtml($attachment.Properties.getPropertyFormatted("DAV:displayname"))</a>
										#end
										
										#if (!$props.getBooleanProperty($props.NamePropIsCollection))
												($props.getPropertyFormatted($props.NamePropContentLength))
										#end
									#end
									</li>
									#set ($props = false)
								#end
							</ul>
						#end
						#if ($size == 0) 
							## $tlang.getString("gen.noattach")
							<div class="act">
								<a href="#" onclick="javascript:document.getElementById('createForm').action='#toolLinkParam("ResourcesAction" "doCreateitem" "special=upload&flow=linkResource&field=$validator.escapeUrl($tag.dottedname)")';submitform('createForm');">Link to a resource</a>
							</div>
						#else
							<div class="act">
								<a href="#" onclick="javascript:document.getElementById('createForm').action='#toolLinkParam("ResourcesAction" "doCreateitem" "special=upload&flow=linkResource&field=$validator.escapeUrl($tag.dottedname)")';submitform('createForm');">Link to a resource</a>
							</div>
						#end
						
					#else
						$tag.dottedname<br />
						$tag.widget<br />
						"$!{tag.value}"<br />
					#end
				</div>
			#end
		#end
	#end
#end
############################################# end of macro getPropertyValues 

